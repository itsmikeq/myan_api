= form_for @custom_route do |f|
  - if @custom_route.errors.any?
    #error_explanation
      %h2= "#{pluralize(@custom_route.errors.count, "error")} prohibited this custom_route from being saved:"
      %ul
        - @custom_route.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :owner_id
    = f.number_field :owner_id
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :owner_type
    = f.text_field :owner_type
  .field
    = f.label :data
    = f.text_field :data
  .field
    = f.label :version
    = f.number_field :version
  .actions
    = f.submit 'Save'
